{
  "assets": [
    "/scripts"
  ],
  "nunjucksFilters": [
    "/filters.js"
  ],
  "nunjucksFunctions": [
    "/functions.js"
  ],
  "nunjucksPaths": [
    "/views",
    "/macros"
  ],
  "scripts": [
    "/scripts/foo.js",
    {
      "path": "/scripts/foo-module.js",
      "type": "module"
    }
  ],
  "sass": [
    "/sass/foo.scss"
  ],
  "nunjucksMacros": [
    {
      "importFrom": "foo-field.njk",
      "macroName": "fooField"
    }
  ]
}
